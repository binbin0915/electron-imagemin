{
  "name": "electron-imagemin",
  "version": "1.0.0",
  "description": "A imagemin Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager ./ 图片压缩精灵 --all --out ./build",
    "build-win": "electron-packager ./ 图片压缩精灵 --platform=win32 --arch=x64 --out ./build --overwrite",
    "build-os": "electron-packager ./ 图片压缩精灵 --platform=darwin --arch=x64 --out ./build"
  },
  "repository": "https://github.com/leeseean/electron-imagemin",
  "keywords": [
    "Electron",
    "tutorial",
    "imagemin"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^8.2.3"
  },
  "dependencies": {
    "compressorjs": "^1.0.6"
  }
}
